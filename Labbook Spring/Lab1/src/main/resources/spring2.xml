<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
		http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-4.3.xsd">
	<bean id="emp" class="com.cg.iter.lab1.b.Employee" autowire="byType">
	<property name="employeeId" value="1234"></property>
	<property name="employeeName" value="Ganesh"></property>
	<property name="salary" value="500000"></property>
	<property name="age" value="21"></property>
	</bean>
	<bean id="sbu" class="com.cg.iter.lab1.b.SBU">
	<property name="sbuId" value="420"></property>
	<property name="sbuName" value="MARKETING"></property>
	<property name="sbuHead" value="Simuel K"></property>
	</bean>

</beans>
